{
  "reporter": "raw",
  "watch": false,
  "rules": {
    "useMixinInsteadUnit": {
      "conf": "never"
    },
    "color": {
      "conf": "lowercase",
      "enabled": true,
      "allowOnlyInVar": true,
      "allowShortcut": true,
      "denyRGB": false
    },
    "mixedSpaces": {
      "indentPerf": "spaces"
    },
    "depthControl": {
      "enabled": false,
      "indentPref": 2
    }
  }
}